{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Edgarborras94/laravel-comments.git"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "authy/php": "4.0.0-rc2",
        "beyondcode/laravel-comments": "^1.2",
        "cybercog/laravel-love": "^8.7",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "gusapi/gusapi": "^5.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.12",
        "laravel/octane": "^1.0",
        "laravel/sanctum": "^2.9",
        "laravel/socialite": "^5.2",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "^1.1",
        "mpociot/teamwork": "^6.1",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-tags": "^3.1",
        "spiral/roadrunner": "^2.0",
        "timehunter/laravel-google-recaptcha-v3": "^2.5",
        "twilio/sdk": "^6.26",
        "willvincent/laravel-rateable": "^2.3"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true

    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
